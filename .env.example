# App Settings
APP_NAME=FranciscoSolis
APP_ENV=local
VITE_APP_ENV="${APP_ENV}"
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Log Channel
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database
DB_CONNECTION=pgsql
DB_HOST=pgsql
DB_PORT=5432
DB_DATABASE=franciscosolis
DB_USERNAME=franciscosolis
DB_PASSWORD=secret

# Caching
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=database
